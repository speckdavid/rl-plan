(define (problem strips-gripper-x-0)
   (:domain gripper-strips)
   (:objects rooma roomb ball1 left right)
   (:init (room rooma)
          (room roomb)
          (ball ball1)
          (at-robby rooma)
          (free right)
          (at ball1 rooma)
          (gripper right))
   (:goal (and
               (at ball1 roomb))))
